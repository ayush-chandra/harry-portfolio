    display: flex !important

\::-webkit-scrollbar
  width: 10px

\::-webkit-scrollbar-track
  inset: 0 0 6px rgba(0, 0, 0, 0.3)
  border-radius: 10px

\::-webkit-scrollbar-thumb
  background: var(--primary-low)
  border-radius: 5px

  &:hover
