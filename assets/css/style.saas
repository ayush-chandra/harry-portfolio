.welcome-area
  overflow: hidden
  position: relative
  display: flex
  align-items: center
  justify-content: center
  height: 100vh
  background-repeat: no-repeat
  background-position: center center
  background-size: cover

  .header-text
    position: absolute
    top: 55%
    transform: translateY(-55%)
    text-align: center
    width: 100%

    h1
