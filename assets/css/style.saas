    position: absolute
    width: 140%
    height: 140%
    opacity: 0.95
    background-repeat: no-repeat
    background-size: cover
    background-position: center center
    z-index: 2
    top: -20%
    left: -20%

  .mini-content
    position: relative
    z-index: 3

    .info
      text-align: center
      color: #fff
      padding-top: 20px
      padding-bottom: 20px

      h1
        color: var(--primary-high)
        font-weight: 500
        font-size: 28px
        letter-spacing: 0.25px
        margin-bottom: 30px

      p
        letter-spacing: 1px
        margin-bottom: 40px
        color: #767676
        letter-spacing: 0.25px
        line-height: 26px
        font-weight: 400
        font-size: 15px

    .mini-box
      display: block
      background: #fff
      padding: 20px
      border-radius: 20px
      box-shadow: 0 2px 48px 0 rgba(0, 0, 0, 0.08)
      margin-bottom: 30px
      position: relative
      -webkit-transition: all 0.3s ease 0s
      -moz-transition: all 0.3s ease 0s
      -o-transition: all 0.3s ease 0s
      transition: all 0.3s ease 0s
      text-align: center

      &:hover
        margin-top: -10px
