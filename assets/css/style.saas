    margin-bottom: 100px

.padding-bottom-100
  padding-bottom: 70px !important

.align-self-center
  -ms-flex-item-align: center !important
  align-self: center !important

.padding-bottom-0
  padding-bottom: 0 !important

.padding-top-70
  padding-top: 70px !important

.margin-bottom-30
  margin-bottom: 30px !important

@media (max-width: 991px)
  body, html
    overflow-x: hidden

  .mobile-top-fix
    margin-top: 30px
    margin-bottom: 0

  .mobile-bottom-fix
    margin-bottom: 30px

  .mobile-bottom-fix-big
    margin-bottom: 60px

@media (max-width: 768px)
  .mobile-bottom-fix-big
    margin-bottom: 30px

a
  &.main-button-slider
    display: inline-block
    font-size: 16px
    border-radius: var(--bod-rad-pri)
    padding: 14px 28px
    background-color: var(--primary-high)
    color: #fff
    letter-spacing: 0.3px
    box-shadow: 0 20px 60px rgba(0, 0, 0, 0.25)
    -webkit-transition: all 0.2s ease 0s
    -moz-transition: all 0.2s ease 0s
    -o-transition: all 0.2s ease 0s
    transition: all 0.2s ease 0s

    &:hover
      background-color: var(--cta)
      box-shadow: 0 10px 20px -20px rgba(0, 0, 0, 0.25)
      transform: translate(0, 2.5px)

  &.main-button
    font-size: 13px
    border-radius: var(--bod-rad-sec)
    padding: 12px 20px
    background: var(--primary-low)
    text-transform: uppercase
    color: #fff
    letter-spacing: 0.5px
    -webkit-transition: all 0.3s ease 0s
    -moz-transition: all 0.3s ease 0s
    -o-transition: all 0.3s ease 0s
    transition: all 0.3s ease 0s

    &:hover
      background: var(--primary-high)

button.main-button
  outline: 0
  border: none
  cursor: pointer
  font-size: 13px
  border-radius: 3px
  padding: 12px 20px
  background: var(--primary-high)
  color: #fff
  letter-spacing: 1px
  -webkit-transition: all 0.3s ease 0s
  -moz-transition: all 0.3s ease 0s
  -o-transition: all 0.3s ease 0s
  transition: all 0.3s ease 0s
  box-shadow: 0 10px 20px 0 rgba(0, 0, 0, 0.1)

  &:hover
    background: var(--cta)
    box-shadow: 0 0 0 0 rgba(0, 0, 0, 0.1)

\:root
  --cta:#000000
  --primary-high:#333333
  --primary-low:#4f4f4f
  --secondary-high:#767676
  --secondary-low:#e0e0e0
  --base:#ffffff
  --base-alpha:#fafafa
  --bod-rad-pri:0px
  --bod-rad-sec:5px

.arrow
  position: absolute
  left: 50%

  &:before
    position: absolute
    left: 50%
    content: ''
    width: 20px
    height: 20px
    top: 50%
    margin: -10px 0 0 -10px
    border-left: none
    border-top: none
    border-right: 1px #333 solid
    border-bottom: 1px #333 solid
    animation-duration: 2s
    animation-iteration-count: infinite
    animation-name: arrow

  width: 40px
  height: 40px
  margin: 45px 0 0 -20px
  -webkit-transform: rotate(45deg)
  transform: rotate(45deg)
  border-left: none
  border-top: none
  border-right: 2px #333 solid
  border-bottom: 2px #333 solid

@media (max-width: 765px)
  .arrow
    margin: 45px 0 0 -20px

@keyframes arrow
  0%
    opacity: 1

  100%
    opacity: 0
    transform: translate(-10px, -10px)

.header-area
  position: fixed
  top: 30px
  left: 0
  right: 0
  z-index: 100
  height: 100px
  -webkit-transition: all 0.3s ease 0s
  -moz-transition: all 0.3s ease 0s
  -o-transition: all 0.3s ease 0s
  transition: all 0.3s ease 0s

  .main-nav
    min-height: 80px

    .logo
      float: left
      margin-top: 22.5px
      -webkit-transition: all 0.3s ease 0s
      -moz-transition: all 0.3s ease 0s
      -o-transition: all 0.3s ease 0s
      transition: all 0.3s ease 0s
      margin-left: 30px

      img
        -webkit-transition: all 0.3s ease 0s
        -moz-transition: all 0.3s ease 0s
        -o-transition: all 0.3s ease 0s
        transition: all 0.3s ease 0s

    .nav
      float: right
      margin-top: 27px
      margin-left: 0
      margin-right: 30px
      -webkit-transition: all 0.3s ease 0s
      -moz-transition: all 0.3s ease 0s
      -o-transition: all 0.3s ease 0s
      transition: all 0.3s ease 0s
      position: relative
      z-index: 999

      li
        padding-left: 20px
        padding-right: 20px

        &:last-child
          padding-right: 0

        a
          display: block
          font-weight: 500
          font-size: 12px
          color: #fff
          -webkit-transition: all 0.3s ease 0s
          -moz-transition: all 0.3s ease 0s
          -o-transition: all 0.3s ease 0s
          transition: all 0.3s ease 0s
          height: 40px
          line-height: 40px
          border: transparent
          letter-spacing: 1px

          &:hover
            color: var(--cta)

    .menu-trigger
      cursor: pointer
      display: block
      position: absolute
      top: 23px
      width: 32px
      height: 40px
      text-indent: -9999em
      z-index: 99
      right: 20px
      display: none

      span
        -moz-transition: all 0.4s
        -o-transition: all 0.4s
        -webkit-transition: all 0.4s
        transition: all 0.4s
        background-color: #3b566e
        display: block
        position: absolute
        width: 30px
        height: 2px
        left: 0

        &:after, &:before
          -moz-transition: all 0.4s
          -o-transition: all 0.4s
          -webkit-transition: all 0.4s
          transition: all 0.4s
          background-color: #3b566e
          display: block
          position: absolute
          width: 30px
          height: 2px
          left: 0

        &:after, &:before
          -moz-transition: all 0.4s
          -o-transition: all 0.4s
          -webkit-transition: all 0.4s
          transition: all 0.4s
          background-color: #3b566e
          display: block
          position: absolute
          width: 30px
          height: 2px
          left: 0
          width: 75%

        &:after
          content: ''

        &:before
          content: ''
          -moz-transform-origin: 33% 100%
          -ms-transform-origin: 33% 100%
          -webkit-transform-origin: 33% 100%
          transform-origin: 33% 100%
          top: -10px
          z-index: 10

        top: 16px

        &:after
          -moz-transform-origin: 33% 0
          -ms-transform-origin: 33% 0
          -webkit-transform-origin: 33% 0
          transform-origin: 33% 0
          top: 10px

      &.active span
        background-color: transparent
        width: 100%

        &:after
          background-color: transparent
          width: 100%

        &:before
          background-color: transparent
          width: 100%
          -moz-transform: translateY(6px) translateX(1px) rotate(45deg)
          -ms-transform: translateY(6px) translateX(1px) rotate(45deg)
          -webkit-transform: translateY(6px) translateX(1px) rotate(45deg)
          transform: translateY(6px) translateX(1px) rotate(45deg)
          background-color: #3b566e

        &:after
          -moz-transform: translateY(-6px) translateX(1px) rotate(-45deg)
          -ms-transform: translateY(-6px) translateX(1px) rotate(-45deg)
          -webkit-transform: translateY(-6px) translateX(1px) rotate(-45deg)
          transform: translateY(-6px) translateX(1px) rotate(-45deg)
          background-color: #3b566e

  &.header-sticky
    min-height: 80px

    .nav
      margin-top: 20px !important

      li a
        color: var(--secondary-high)

        &.active
          color: var(--cta)
          font-weight: 600

@media (max-width: 768px)
  .header-area
    top: 20px

@media (min-width: 768px)
