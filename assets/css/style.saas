.counter
  overflow: hidden
  position: relative

  &:before
    content: ''
    position: absolute
    width: 140%
    height: 140%
    background: #333
    background-repeat: no-repeat
    background-size: cover
    background-position: center center
    z-index: 2
    top: -20%
    left: -20%
    transition: background 1s

  &:hover:before
    background: var(--cta)

  .content
    position: relative
    z-index: 3
    width: 100%

    .count-item
      height: 340px
      position: relative
      float: left
      width: 100%

      &.decoration-bottom
        position: relative

        &:after
          content: ''
          position: absolute
          width: 70%
          height: 32px
          top: 160px
          left: 70%
          background: url(../images/circle-dec.png) center center no-repeat
          -webkit-transition: all 0.3s ease 0s
          -moz-transition: all 0.3s ease 0s
          -o-transition: all 0.3s ease 0s
          transition: all 0.3s ease 0s

      &.decoration-top
        position: relative

        &:after
          content: ''
          position: absolute
          width: 70%
          height: 32px
          top: 140px
          left: 70%
          background: url(../images/circle-dec.png) center center no-repeat
          -webkit-transition: all 0.3s ease 0s
          -moz-transition: all 0.3s ease 0s
          -o-transition: all 0.3s ease 0s
          transition: all 0.3s ease 0s

      &:hover strong
        margin-top: 110px

      strong
        display: block
        text-align: center
        font-weight: 600
        font-size: 36px
        letter-spacing: 0.25px
        margin-bottom: 10px
        color: #fff
        margin-top: 120px
        -webkit-transition: all 0.3s ease 0s
        -moz-transition: all 0.3s ease 0s
        -o-transition: all 0.3s ease 0s
        transition: all 0.3s ease 0s

      span
        display: block
        text-align: center
        color: #fff
        font-weight: 500
        font-size: 17px
        letter-spacing: 0.25px

@media (max-width: 991px)
  .counter
    padding-top: 60px
    padding-bottom: 60px

    .content
      position: relative !important
      top: 0 !important
      transform: perspective(1px) translateY(0) !important

      .count-item
        height: auto
        padding-top: 20px
        padding-bottom: 20px

        &:hover strong
          margin-top: 0

        &:before, &:after
          display: none

        strong
          margin-top: 0

